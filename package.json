{
  "name": "astral-forge",
  "version": "1.0.0",
  "description": "Forge your strength with progressive, adaptive training - automated progressive overload and RPE-based autoregulation",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4001",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
